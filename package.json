{
  "name": "ticketbottle",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "schema": "./prisma/schema",
    "seed": "ts-node --transpile-only --project tsconfig.json prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production dotenv -e .env.prod node dist/main",
    "docker:dev": "docker compose --env-file ./.env.dev -f docker-compose.dev.yml up",
    "docker:prod": "docker compose --env-file ./.env.prod -f docker-compose.prod.yml up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix  ",
    "db:restart": "docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.dev.yml up -d && sleep 1",
    "seed": "prisma db seed",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --detectOpenHandles --watch",
    "test:cov": "cross-env NODE_ENV=test jest --detectOpenHandles --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretest:int": "cross-env NODE_ENV=test npm run db:restart && cross-env NODE_ENV=test prisma migrate reset --force  && cross-env NODE_ENV=test ts-node prisma/seed.test.ts",
    "test:int": "cross-env NODE_ENV=test jest -i --no-cache --watch --config jest-int.json",
    "posttest:int": "cross-env NODE_ENV=test prisma migrate reset --force",
    "migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev"
  },
  "dependencies": {
    "@bull-board/api": "^6.5.3",
    "@bull-board/express": "^6.5.3",
    "@bull-board/nestjs": "^6.5.3",
    "@faker-js/faker": "^9.3.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^2.3.0",
