    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-themes": "^1.4.3",
    "vnpay": "^1.6.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
