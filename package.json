    "joi": "^17.13.3",
    "madge": "^8.0.0",
    "meilisearch": "^0.48.2",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.9.7",
    "nestjs-meilisearch": "^2.1.8",
    "nestjs-vnpay": "^0.1.0",
