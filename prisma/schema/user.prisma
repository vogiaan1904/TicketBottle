enum Gender {
    FEMALE
    MALE
    CUSTOM
}

enum StaffRole {
    ADMIN
    STAFF
}

model User {
    id          String  @id @default(uuid())
    email       String  @unique
    password    String
    firstName   String
    lastName    String
    gender      Gender
    phoneNumber String?
    orders      Order[]
    isVerified  Boolean @default(false)

    @@map("users")
}
